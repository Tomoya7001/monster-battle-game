{
  "version": "4.0",
  "lastUpdated": "2026-01-04",
  "changelog": {
    "v4.0": [
      "【後攻装備追加】EQ907 不屈の心得（後攻時状態異常無効・1回）",
      "【後攻装備追加】EQ908 反撃の刃（後攻時クリティカル率+2段階）"
    ],
    "v3.0": [
      "【包括的バランス調整】人間装備制限、後攻有利装備追加",
      "【人間装備制限】EQ002/EQ004/EQ007 火力装備にhuman制限追加",
      "【人間装備制限】EQ201-207 属性エンブレムにhuman制限追加",
      "【新規追加】EQ904 鈍重の誓い（後攻時攻撃×1.2）",
      "【新規追加】EQ905 不動の構え（後攻時被ダメ×0.85）",
      "【新規追加】EQ906 逆転の兆し（HP半分以下で先制権）",
      "【強化】EQ903 先制封じの札に相手SPD-10追加"
    ],
    "v2.2": [
      "【新規追加】EQ901 時空の砂時計（速度対策：行動順逆転）",
      "【新規追加】EQ902 重力の腕輪（速度対策：相手速度-20%）",
      "【新規追加】EQ903 先制封じの札（速度対策：先制攻撃無効化）"
    ],
    "v2.1": [
      "【新規追加】EQ111 解放の指輪（封印無効）",
      "【カウント修正】metadata全体を実数に合わせて修正"
    ],
    "v2.0": [
      "【構造変更】speciesRestriction → excludedSpecies（使用不可種族を明確化）",
      "【用語統一】bind → restraint（アビリティマスターと統一）",
      "【運要素排除】賭博師のダイス: ランダム倍率 → HP条件倍率に変更",
      "【新規追加】EQ110 清浄の指輪（瘴気無効）",
      "【新規追加】EQ609 怨念の瘴気（倒れた時瘴気付与）",
      "【新規追加】EQ610 怨念の封印（倒れた時封印付与）"
    ],
    "v1.2": [
      "撤退の護符、鈍足の靴を追加"
    ]
  },
